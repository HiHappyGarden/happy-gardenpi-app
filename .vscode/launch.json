	"version": "0.2.0",
    "configurations": [
        {
            "type": "gdb",
            "request": "attach",
            "name": "Attach to gdbserver",
            "gdbpath": "/usr/bin/gdb-multiarch",
            "executable": "${workspaceFolder}/build/hgardenpi",
            "target": "passy-raspberrypi:3000",
            "remote": true,
            "printCalls": true,
            "cwd": "${workspaceRoot}/build/",
            "valuesFormatting": "parseText"
        }
    ]
}